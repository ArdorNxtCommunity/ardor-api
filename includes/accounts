            </span><span class="s2">"attachment"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
                </span><span class="s2">"version.OrdinaryPayment"</span><span class="p">:</span><span class="w"> </span><span class="mi">0</span><span class="w">
            </span><span class="p">},</span><span class="w">
            </span><span class="s2">"senderRS"</span><span class="p">:</span><span class="w"> </span><span class="s2">"ARDOR-HWZW-5TT6-U68F-H26L8"</span><span class="p">,</span><span class="w">
            </span><span class="s2">"subtype"</span><span class="p">:</span><span class="w"> </span><span class="mi">0</span><span class="p">,</span><span class="w">
            </span><span class="s2">"amountNQT"</span><span class="p">:</span><span class="w"> </span><span class="s2">"500000000"</span><span class="p">,</span><span class="w">
            </span><span class="s2">"sender"</span><span class="p">:</span><span class="w"> </span><span class="s2">"17530402417794348028"</span><span class="p">,</span><span class="w">
            </span><span class="s2">"recipientRS"</span><span class="p">:</span><span class="w"> </span><span class="s2">"ARDOR-492G-A5YD-FUGZ-C6N2B"</span><span class="p">,</span><span class="w">
